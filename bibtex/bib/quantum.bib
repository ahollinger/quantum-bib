@InProceedings{BC91,
  Title                    = {Quantum Bit Commitment and Coin Tossing Protocols},
  Author                   = {Brassard, Gilles and Cr\'{e}peau, Claude},
  Booktitle                = {Advances in Cryptology - CRYPT0 1990},
  Year                     = {1991},
  Pages                    = {49--61},
  Publisher                = {Springer},
  Series                   = {LNCS},

  Abstract                 = {In the late 1960{\textquoteright}s a physicist, Stephen Wiesner, had the idea that the uncertainty principle could be used for cryptography (though he published his result much later [Wie83]). One of his ideas was that it would be possible to use a stream of polarized photons to transmit two messages in a way that would make only one of them readable at the receiver{\textquoteright}s choosing. This notion, which he called {\textquotedblleft}multiplexing{\textquotedblright}, is remarkably similar to the {\textquotedblleft}one-out-of-two oblivious transfer{\textquotedblright} to be reinvented many years later [EGL83], and it even predates Rabin{\textquoteright}s notion of oblivious transfer [Rab81] by more than a decade. In the late 1970{\textquoteright}s, Wiesner{\textquoteright}s invention was brought back to life by the work of Charles H. Bennett and Gilles Brassard, which resulted in a CRYPTO {\textquoteright}82 paper [BBBW82]. Subsequently, Bennett and Brassard used quantum cryptographic principles to implement basic cryptographic protocols, such as secret key exchange and coin tossing by telephone [BB84]. There has been recently much excitement in the field of quantum cryptography because a working prototype of the quantum key exchange channel has been successfully built at the IBM T. J. Watson Research Laboratory, Yorktown Heights [BBBSS90].},
  Doi                      = {10.1007/3-540-38424-3\_4},
  ISBN                     = {3-540-54508-5},
  Url                      = {http://dl.acm.org/citation.cfm?id=646755.705216}
}

