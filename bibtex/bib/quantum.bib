@InProceedings{Liu14b,
  Title                    = {Single-Shot Security for One-Time Memories in the Isolated Qubits Model},
  Author                   = {Liu, Yi-Kai},
  Booktitle                = {Advances in Cryptology – CRYPTO 2014},
  Year                     = {2014},
