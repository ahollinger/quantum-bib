@Article{BCMS97,
  Title                    = {A brief review on the impossibility of quantum bit commitment},
  Author                   = {Brassard, Gilles and Cr\'{e}peau, Claude and Mayers, Dominic and Salvail, Louis},
  Journal                  = {arXiv preprint quant-ph/9712023},
